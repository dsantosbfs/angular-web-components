{
  "name": "web-lib",
  "version": "0.0.0",
  "scripts": {
    "build:lib": "ng build lib",
    "build:web-components": "ng build web-components --prod --optimization=true --output-hashing=none && ./sh/web-components.sh",
    "build:storybook": "build-storybook -c .storybook -o dist/storybook",
    "e2e": "ng e2e",
    "lint": "ng lint lib",
    "lint:fix": "ng lint lib --fix && sass-lint-auto-fix",
    "lint:sass": "sass-lint -v -q",
    "list-todos": "./sh/list-todos.sh",
    "ng": "ng",
    "pack:lib": "cd dist/lib && npm pack",
    "start": "ng serve",
    "storybook": "start-storybook -s ./src/assets -p 9000",
    "test": "ng test --code-coverage --watch=false --sourceMap=true  --browsers=ChromeHeadless",
    "test:lib": "ng test lib --code-coverage --sourceMap=true",
    "validate-pr": "./sh/validate-pr.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run lint:sass && npm run list-todos",
      "pre-push": "npm run test"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.2",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/elements": "^8.1.3",
    "@angular/forms": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "@types/chart.js": "^2.7.56",
    "@webcomponents/webcomponentsjs": "^2.4.0",
    "chart.js": "^2.8.0",
    "chartjs-plugin-datalabels": "^0.6.0",
    "document-register-element": "^1.7.2",
    "font-awesome": "^4.7.0",
    "ng2-charts": "^2.3.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@babel/core": "^7.5.5",
    "@storybook/addon-a11y": "^5.1.9",
    "@storybook/addon-centered": "^5.1.10",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-notes": "^5.1.9",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/angular": "^5.1.9",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "babel-loader": "^8.0.6",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-spec-reporter": "0.0.32",
    "lint-staged": "^9.2.0",
    "protractor": "~5.4.0",
    "sass-lint": "^1.13.1",
    "sass-lint-auto-fix": "0.17.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
